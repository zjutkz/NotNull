{
    "name": "notNull",
    "version": "1.0.0",
    "description": "A ts decorator that avoid undefined parameters",
    "author": "zjutkz",
    "scripts": {
        "build": "npm run clean && tsc",
        "dev": "npm run clean && tsc -w",
        "prepublish": "npm run build",
        "start": "tsc -w"
    },
    "dependencies": {
        "reflect-metadata": "^0.1.13"
    },
    "devDependencies": {},
    "keywords": [],
    "license": "MIT",
    "directories": {
        "doc": "doc"
    },
    "main": "./index.js",
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "exclude": [
            "**/*.d.ts",
            "coverage/**",
            "test/**",
            "demo/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true
    }
}
